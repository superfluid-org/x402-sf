{
  "name": "super-x402",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm --filter @super-x402/website build",
    "build:facilitator": "pnpm --filter @super-x402/facilitator build",
    "build:website": "pnpm --filter @super-x402/website build",
    "build:all": "pnpm build:facilitator && pnpm build:website",
    "dev:website": "pnpm --filter @super-x402/website dev"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@types/uuid": "^9.0.8"
  }
}
